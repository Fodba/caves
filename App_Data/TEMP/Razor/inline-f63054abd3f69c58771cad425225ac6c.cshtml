@using umbraco.MacroEngines
		@using umbraco.NodeFactory
		@inherits umbraco.MacroEngines.DynamicNodeContext
		@{
		try{
			var link = "style=\"background:url(" + Model.NodeById(Model.backgroundImage).Url + ") no-repeat center center fixed;background-size:cover\">";
		@Html.Raw(link)}
		catch{
			var link=">"
		@Html.Raw(link)
		}
		}